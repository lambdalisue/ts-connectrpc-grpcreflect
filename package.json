{
  "name": "connectrpc-grpcreflect-es",
  "version": "0.0.0",
  "description": "A connect-es module for gRPC reflection",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/lambdalisue/connectrpc-grpcreflect-es#readme",
  "bugs": {
    "url": "https://github.com/lambdalisue/connectrpc-grpcreflect-es/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lambdalisue/connectrpc-grpcreflect-es.git"
  },
  "author": "Alisue <lambdalisue@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "gen": "buf generate && buf build -o src/_gen/file_descriptor.binpb",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "fmt": "prettier --write .",
    "release": "bumpp && npm publish"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^22.15.17",
    "bumpp": "^10.1.0",
    "eslint": "^9.32.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.6.2",
    "@connectrpc/connect": "^2.0.3"
  }
}
